{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "es2022",
    "target": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "types": [
      "bun-types",
      "vitest/globals"
    ],
  }
}
